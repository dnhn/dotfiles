[alias]
  alias = !git config --file ~/.git-alias --get-regexp ^alias\\.
  aa = add .
  ap = add -p
  bd = bundle
  br = branch -v
  brr = branch -rv
  bra = branch -av
  brm = branch -m
  brd = branch -D
  brdr = branch -dr
  cl = clean
  clfd = clean -fd .
  cm = commit
  cmm = commit -m
  cma = commit --amend
  cman = commit --amend --no-edit
  cms = commit -S
  cmms = commit -S -m
  cmas = commit -S --amend
  cmans = commit -S --amend --no-edit
  count = rev-list --count --all
  co = checkout
  cob = checkout -b
  coo = checkout --
  df = diff
  dfc = diff --cached
  dfstg = diff --staged
  dft = difftool
  dfto = difftool --tool=opendiff
  ft = fetch
  ftpr = fetch --prune
  lg = log -n 20 --graph --oneline --decorate=full --branches=* --remotes=*
  lgsst = lg --shortstat
  lgst = lg --stat
  pl = pull
  pol = pull origin
  polr = pull origin --rebase
  ps = push
  pos = push origin
  rbs = rebase
  rem = reset HEAD --
  remote = remote -v
  rev = reset HEAD^
  sh = show
  slog = shortlog -sn --all
  sloge = shortlog -sne --all
  ss = stash
  ssa = stash apply
  ssd = stash drop
  ssh = stash show -p
  ssl = stash list
  ssu = stash save -u
  ssp = stash pop
  sspu = stash push -u
  sss = stash save
  st = status -bs
  stf = status -v --show-stash
