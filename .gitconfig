[core]
  eol = lf
[alias]
  bd = bundle
  br = branch
  cl = clean
  cm = commit
  cma = commit --amend
  cman = commit --amend --no-edit
  cms = commit -S -m
  count = rev-list --count --all
  co = checkout
  coo = checkout --
  df = diff
  ft = fetch
  lg = log -n 20 --graph --oneline --decorate=full
  pl = pull
  ps = push
  pol = pull origin
  pos = push origin
  rem = reset HEAD --
  sh = show
  slog = shortlog -sn --all
  sloge = shortlog -sne --all
  ss = stash
  ssu = stash -u
  ssp = stash pop
  sss = stash save
  st = status
[color]
  ui = true
[user]
  name = Nhan Doan
  email = ""
  signingkey = ""
